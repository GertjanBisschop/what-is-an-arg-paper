# Params for Drosophila inference
NE = 1e6
# Values below from stdpopsim
MU=5.49e-09
RHO = 8.4e-09


KWARG_GITREPO = https://github.com/a-ignatieva/kwarg
KWARG_VERSION = v1.0
# don't allow sequencing errors or recurrent mutations; set seed to 9999
KWARG_PARAMS = -S-1 -M-1 -Z9999


ARGWEAVER_GITREPO = https://github.com/CshlSiepelLab/argweaver
# ARGweaver doesn't have a tagged release, so use the latest as of Mar 2022
ARGWEAVER_VERSION = 7191343c1d43c02de04e4e0a150d77b0612728c6
ARGWEAVER_PARAMS = --smc-prime --popsize %(NE)s --mutrate %(MU)s --recombrate %(RHO)s --randseed 111 --iters 3 --sample-step 10000 --no-compress-output


RELATE_GITREPO = https://github.com/MyersGroup/relate
# Relate doesn't have a tagged release, so use the latest as of Mar 2022
RELATE_VERSION = eea4a89b1d5eba5561f64d2a96eb64aaab6cd91c
# NB: RHO is used to construct a genetic map which is passed in separately to Relate
RELATE_PARAMS = --mode All -m %(MU)s -N %(NE)s --seed 111


RELATELIB_GITREPO = https://github.com/leospeidel/relate_lib
RELATELIB_VERSION = O0b461f3c7282d784e5a65122c029bf4a857cbee5
RELATELIB_PARAMS =


TSINFER_GITREPO = https://github.com/tskit-dev/tsinfer
TSINFER_VERSION = 0.3.1